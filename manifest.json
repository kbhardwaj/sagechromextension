{
  
  "manifest_version": 2,
  
  "name": "SageMob Extension",
  "version": "1.0",
  "description": "Learn new skills and share the latest resources.",
  
  "icons": { "16" : "images/brain-icon.png",
 			"48" : "images/48.png",
			"128" : "images/128.png"},
  
  "permissions": [
	  "http://localhost:3000/api/v1/notebooks.json/",
	  "http://localhost:3000/api/v1/tokens.json/",
	  "http://localhost:3000/*"

	],

  "browser_action": {
    "default_icon": "images/brain-icon.png",     
    "default_popup": "popup.html"       
  }
}
